# https://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line=lf
insert_final_newline=true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true
max_line_length = 150

[*.md]
trim_trailing_whitespace = false
indent_size = 2

[{*,*.yml,*.json,*.js,*.ts,*.vue,*.html,*.htm,*.sh,*.less,*.css,nginx.conf}]
indent_size = 2
